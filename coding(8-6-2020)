importjava.util.Scanner;
classFloydTriangle
{ 
publicstaticvoidmain(Stringargs[]) 
{
intn,num=1,c,d; 
Scannerin=newScanner(System.in); 
System.out.println("EnterthenumberofrowsofFloyd'striangletodisplay");
n=in.nextInt();
System.out.println("Floyd'striangle:");
for(c=1;c<=n;c++)
{ 
for(d=1;d<=c;d++)
{
System.out.print(num+"");
num++; 
}
System.out.println();
}
}
}
